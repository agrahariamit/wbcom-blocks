{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wbcom-blocks/wbcom-heading",
  "version": "1.0.0",
  "title": "Wbcom Heading",
  "category": "wbcom-designs",
  "icon": "heading",
  "description": "Heading Block",
  "example": {},
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "h2"
    }
  },
  "supports": {
    "html": false,
    "splitting": true,
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "textAlign": true
    },
    "color": {
      "gradients": true
    },
    "shadow": {
      "presets": [
        {
          "name": "Primary",
          "slug": "primary",
          "shadow": "16px 16px var( --wp--preset--color--primary )"
        }
      ]
    },
    "align": true,
    "position": {
      "sticky": true,
      "top": "0px"
    },
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "textdomain": "wbcom-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}